{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs22.x"
      }
    }
  ],
  "buildCommand": "npm run build && npx prisma generate",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ]
}
